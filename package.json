{
  "name": "fdx-client",
  "version": "0.2.0",
  "description": "Node.js client for Feedonomics API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --options typedoc.json",
    "example:manual-bigcommerce-setup": "ts-node src/examples/bigcommerce-manual-setup.ts",
    "example:automated-bigcommerce-setup": "ts-node src/examples/bigcommerce-automated-setup.ts",
    "example:verify-api-token": "ts-node src/examples/verify-token.ts",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "author": "Nate Stewart <nate.stewart@bigcommerce.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.3",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.28.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
